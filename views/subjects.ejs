<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard | Subjects</title>
    <link rel="stylesheet" href="/dist/subjects.css">
</head>

<body>
    <%- include('partials/navbar', {subjects: true}) %>
        <main class="container mt-5">
            <header class="mb-4">
                <h1 class="fs-3 text-center">Subjects</h1>
            </header>
            <main>
                <div class="row container px-5">
                    <% courses.forEach(course => { %>
                    <div class="col-4 mb-5 px-4">
                        <%- include('partials/subject', {course}) %>
                    </div>
                    <% }) %>
                </div>
            </main>
        </main>
        <script src="/dist/subjects.js"></script>
</body>

</html>
